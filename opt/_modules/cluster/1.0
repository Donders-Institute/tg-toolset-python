#%Module1.0####################################################################### 
##
## cluster utility modulefile
## 

if { [module-info mode remove] } {
    set name [module-info name]
    puts stderr "Unloading is not allowd: $name"
    break
} else {

    source $env(DCCN_MOD_DIR)/share/common.tcl

    set version 1.0
    set arch [get_sys_arch]

    source [file join [file dirname $ModulesCurrentModulefile] common.tcl]
}
