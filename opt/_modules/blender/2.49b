#%Module1.0####################################################################### 
##
## blender modulefile
##

source $env(DCCN_MOD_DIR)/share/common.tcl

set version 2.49b
set arch [get_sys_arch]

## blender 2.49b requires to run with python 2.6.5
if { ! [ module-info mode whatis ] } {
    if { ! [ is-loaded python/2.6.5 ] } {
        module load python/2.6.5
    }
}

source [file join [file dirname $ModulesCurrentModulefile] common.tcl]
